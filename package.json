  "private": true,
  "scripts": {
    "start": "npm-run-all -l -p devserver devclient",
    "build": "npm-run-all -l -p buildserver buildclient",
    "devclient": "react-scripts start",
    "buildclient": "react-scripts build",
    "devserver": "cross-env NODE_ENV=development nodemon -w server server/server.js",
    "buildserver": "cross-env NODE_ENV=production node server/server.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
