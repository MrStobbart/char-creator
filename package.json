{
  "name": "char-creator",
  "version": "0.1.0",
  "description": "Website to create roleplaying characters.",
  "licence": "Mozilla Public License 2.0",
  "author": "Marek Meyer <marek_meyer@outlook.com>",
  "bugs": {
    "url": "https://github.com/MrStobbart/char-creator/issues",
    "email": "marek_meyer@outlook.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrStobbart/char-creator"
  },
  "private": true,
  "scripts": {
    "start": "npm-run-all -l -p mongodb server client",
    "build": "react-scripts build",
    "mongodb": "mongod --dbpath data/db",
    "client": "react-scripts start",
    "server": "cross-env NODE_ENV=development nodemon --watch 'src/server/*' --exec 'ts-node' '--project' 'tsconfig.node.json' src/server/server.ts",
    "buildServer": "cross-env NODE_ENV=production ts-node --project tsconfig.node.json src/server/server.ts",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "parseRules": "ts-node --project tsconfig.node.json src/server/data/extractRules.ts",
    "lint": "eslint . --ext .ts",
    "fix": "eslint . --ext .ts --fix"
  },
  "dependencies": {
    "@types/react-table": "^6.8.3",
    "app-root-path": "^2.2.1",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.6",
    "connect-history-api-fallback": "^1.5.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "i18next": "^15.1.0",
    "mongodb": "^3.1.8",
    "morgan": "^1.9.1",
    "react": "^16.6.0",
    "react-autosuggest": "^9.4.2",
    "react-dom": "^16.6.0",
    "react-i18next": "^10.9.0",
    "react-markdown": "^4.0.8",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^3.0.0",
    "react-select": "^2.1.1",
    "react-table": "^6.10.0",
    "reinspect": "^1.0.0",
    "shortid": "^2.2.13",
    "winston": "^3.2.1",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/classnames": "^2.2.6",
    "@types/connect-history-api-fallback": "^1.3.2",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.16.1",
    "@types/jest": "^23.3.14",
    "@types/mongodb": "^3.1.26",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.14.6",
    "@types/react": "^16.8.13",
    "@types/react-autosuggest": "^9.3.6",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-select": "^2.0.6",
    "@types/shortid": "0.0.29",
    "@types/xml2js": "^0.4.4",
    "cross-env": "^5.2.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.1",
    "nodemon": "^1.18.5",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.17.0",
    "react-test-renderer": "^16.6.0",
    "request": "^2.88.0",
    "request-animation-frame": "^0.1.1",
    "typescript": "^3.4.5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
