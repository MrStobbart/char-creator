    "react-router-redux": "^5.0.0-alpha.8",
    "react-scripts": "1.0.14",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "cross-env": "^5.1.1",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
